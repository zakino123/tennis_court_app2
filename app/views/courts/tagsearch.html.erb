<%= render 'courts/search_form' %>
<div class="container py-4 ">
  <ul class="mb-2 mx-auto grid gap-x-2 gap-y-4 grid-cols-1 sm:grid-cols-2 md:grid-cols-3 lg:grid-cols-4">
    <% if @tag_court_lists.any? %>
      <% @tag_court_lists.each do |court| %>
        <%= render court %>
      <% end %>
    <% end %>
  </ul>
  <%= paginate @courts %>
</div>